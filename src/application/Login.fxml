<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="155.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.Main_application_controller">
   <children>
      <ImageView fx:id="logo" fitHeight="131.0" fitWidth="132.0" layoutX="14.0" layoutY="11.0" pickOnBounds="true" preserveRatio="true" >
      </ImageView>
      <TextField fx:id="name_field" alignment="CENTER" layoutX="182.0" layoutY="22.0" onKeyPressed="#connect_via_button" prefHeight="38.0" prefWidth="482.0" promptText="Enter your Chatname" />
      <Button fx:id="connect_button" layoutX="301.0" layoutY="91.0" mnemonicParsing="false" onAction="#connect" prefHeight="38.0" prefWidth="190.0" text="CONNECT" />
   </children>
</AnchorPane>
